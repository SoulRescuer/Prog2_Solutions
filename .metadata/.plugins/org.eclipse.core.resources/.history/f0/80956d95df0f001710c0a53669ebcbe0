
public class Karte {
	private Farbe f;
	private Wert w;
	
	public enum Farbe{
		PIK, HERZ, KARO;	
	}
	
	public enum Wert{
		SIEBEN, ACHT, NEUN, ZEHN, BAUER, DAME, KOENIG, ASS;
	}
	
	
	//Karten Konstruktor
	public Karte (Farbe f, Wert w){
		this.f=f;
		this.w=w;
	}
	
	
	
	@Override
	public String toString(){
		return w+ "-" +f;
		
	}
	
	

}
